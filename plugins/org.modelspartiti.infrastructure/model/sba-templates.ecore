  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="array_double" eSuperTypes="#//array">
    <eStructuralFeatures xsi:type="ecore:EReference" name="values" lowerBound="1"
        upperBound="-1" eType="#//value_double" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="multiplicity" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="param">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
