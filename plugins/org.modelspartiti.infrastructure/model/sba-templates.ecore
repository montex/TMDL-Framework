        upperBound="-1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="availability" eSuperTypes="#//metric">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="calculation" lowerBound="1"
        eType="#//availability_calc"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="reliability" eSuperTypes="#//metric">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="calculation" lowerBound="1"
        eType="#//reliability_calc"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="mttf" eSuperTypes="#//metric">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="calculation" lowerBound="1"
        eType="#//mttf_calc"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="availability_calc">
    <eLiterals name="instant_of_time"/>
    <eLiterals name="time_averaged" value="1"/>
    <eLiterals name="steady_state" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="reliability_calc">
    <eLiterals name="instant_of_time"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="mttf_calc">
    <eLiterals name="interval_of_time"/>
  </eClassifiers>
</ecore:EPackage>
