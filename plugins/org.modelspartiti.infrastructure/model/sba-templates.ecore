  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="mergeall" eSuperTypes="#//mergerule">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ref" lowerBound="1" upperBound="-1"
        eType="#//mergeitem" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="mergebyname" eSuperTypes="#//mergerule">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ref" lowerBound="1" upperBound="-1"
        eType="#//mergeitem" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="prefixes" upperBound="-1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="mergeitem">
    <eStructuralFeatures xsi:type="ecore:EReference" name="item" lowerBound="1" eType="#//iface"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="block" eType="#//block"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ids" eType="#//multiplicity"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="value" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="value_int" eSuperTypes="#//value #//multiplicity">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="value_double" eSuperTypes="#//value">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="array" abstract="true" eSuperTypes="#//value"/>
  <eClassifiers xsi:type="ecore:EClass" name="array_int" eSuperTypes="#//array #//multiplicity">
    <eStructuralFeatures xsi:type="ecore:EReference" name="values" upperBound="-1"
