        eType="#//template"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subcomponents" upperBound="-1"
        eType="#//instof" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="instof">
